{
  "name": "musical-fiesta",
  "version": "0.0.7",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "http-server",
    "test": "jest --color --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|scss|sass|ts|tsx|md|mdx|)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run flow",
    "flow": "flow",
    "precommit": ""
  },
  "hooks": {
    "pre-commit": "lint-staged && npm run flow"
  },
  "author": "M. Gildone <matteo.gildone@gmail.com>",
  "license": "MIT",
  "keywords": [
    "npm",
    "storage",
    "frontend"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ciccibus/musical-fiesta.git"
  },
  "bugs": {
    "url": "https://github.com/ciccibus/musical-fiesta/issues"
  },
  "homepage": "https://github.com/ciccibus/musical-fiesta#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "flow-bin": "^0.84.0",
    "http-server": "^0.11.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0"
  }
}
